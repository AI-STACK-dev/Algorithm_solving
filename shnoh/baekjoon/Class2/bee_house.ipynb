   "source": [
    "# 2292번\n",
    "# 1 + 6까지 2\n",
    "# + 12 까지 3\n",
    "#+ 18까지 4\n",
    "# + 24까지 5\n",
    "n = int(input())\n",
    "cnt = 1\n",
    "num = 1\n",
    "mul = 0\n",
    "while True:\n",
    "    num += 6 * mul\n",
    "    if num < n:\n",
    "        cnt += 1\n",
    "        mul += 1\n",
    "    else:\n",
    "        break\n",
    "print(cnt)"
