    "# Q4 만들 수 없는 금액"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "n = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "A.sort() # 오름차순 정렬\n",
    "num = 1 # 불가능 한지 검사하는 수\n",
    "for i in A:\n",
    "    if num<i: # 만들 수 없는 경우\n",
    "        break\n",
    "    else:\n",
    "        num+=i\n",
    "print(num)"
   ]
  }
