   "source": [
    "s = input()\n",
    "n = len(s)\n",
    "m = n / 2\n",
    "front = rear = 0\n",
    "for i in range(n):\n",
    "    if i < m:\n",
    "        front += int(s[i])\n",
    "    else:\n",
    "        rear += int(s[i])\n",
    "if front == rear:\n",
    "    print(\"LUCKY\")\n",
    "else:\n",
    "    print(\"READY\")"
   ]
