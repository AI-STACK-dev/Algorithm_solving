   "source": [
    "n = int(input())\n",
    "A = list(map(int,input().split()))\n",
    "DP = [1]*n\n",
    "for i in range(1,n):\n",
    "    for j in range(i):\n",
    "        if A[j] > A[i]:\n",
    "            DP[i]=max(DP[i],DP[j]+1)\n",
    "print(n-max(DP))\n"
   ]
