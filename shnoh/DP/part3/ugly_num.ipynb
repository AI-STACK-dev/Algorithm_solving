   "source": [
    "n = int(input())\n",
    "DP=[0]*1000\n",
    "DP[0]=1\n",
    "S = set()\n",
    "S.add(1)\n",
    "k=2\n",
    "for i in range(1,n):\n",
    "    while True:\n",
    "        if (k/2 in S) or (k/3 in S) or (k/5 in S):\n",
    "            S.add(k)\n",
    "            DP[i]=k\n",
    "            k+=1\n",
    "            break\n",
    "        k+=1\n",
    "print(DP[n-1])"
   ]
