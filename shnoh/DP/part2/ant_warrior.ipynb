   "source": [
    "n = int(input())\r\n",
    "A = [int(x) for x in input().split()]\r\n",
    "DP= [0]*n\r\n",
    "\r\n",
    "DP[0] = A[0]\r\n",
    "DP[1] = max(A[0],A[1])\r\n",
    "for i in range(2,n):\r\n",
    "    DP[i] = max(DP[i-2]+A[i],DP[i-1])\r\n",
    "print(DP[n-1])"
